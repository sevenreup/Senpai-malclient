<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FadeEdgeImage">
        <attr name="edge" format="integer"/>
        <attr name="edgeDirection" format="enum">
            <enum name="down" value="0"/>
            <enum name="up" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DefaultIndicator">
        <attr name="dotsColor" format="color"/>
        <attr name="dotsSize" format="dimension"/>
        <attr name="dotsSpacing" format="dimension"/>
        <attr name="dotsCornerRadius" format="dimension"/>
        <attr name="dotsStrokeColor" format="color"/>
        <attr name="dotsStrokeWidth" format="dimension"/>
    </declare-styleable>
</resources>